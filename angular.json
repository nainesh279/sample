{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "kibpay": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "apps/kibpay",
      "sourceRoot": "apps/kibpay/src",
      "prefix": "kp",
      "architect": {
        "build": {
          "builder": "@bb-cli/bb-ang:browser",
          "options": {
            "outputPath": "dist/kibpay",
            "index": "apps/kibpay/src/index.html",
            "main": "apps/kibpay/src/main.ts",
            "polyfills": "apps/kibpay/src/polyfills.ts",
            "tsConfig": "apps/kibpay/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "input": "dist/custom-project-assets",
                "glob": "**/*",
                "output": "assets"
              },
              "apps/kibpay/src/logo-header.ico",
              "apps/kibpay/src/assets"
            ],
            "styles": [
              "apps/kibpay/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/kibpay/src/environments/environment.ts",
                  "with": "apps/kibpay/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "shared": {
              "fileReplacements": [
                {
                  "replace": "apps/kibpay/src/environments/environment.ts",
                  "with": "apps/kibpay/src/environments/environment.shared.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kibpay:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "kibpay:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kibpay:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/kibpay/src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "apps/kibpay/src/polyfills.ts",
            "tsConfig": "apps/kibpay/src/../../../tsconfig.spec.json",
            "environment": "dev",
            "codeCoverage": true,
            "codeCoverageExclude": [
              "test.ts",
              "**/polyfills.ts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/kibpay/tsconfig.app.json",
              "apps/kibpay/src/../../../tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "kibpay-e2e": {
      "projectType": "application",
      "prefix": "kp",
      "root": "apps/kibpay-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/kibpay-e2e/protractor.conf.js",
            "devServerTarget": "kibpay:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kibpay:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/kibpay-e2e/tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "widget-kibpay-data": {
      "root": "libs/widget-kibpay-data",
      "sourceRoot": "libs/widget-kibpay-data/src",
      "projectType": "library",
      "prefix": "kp",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/widget-kibpay-data/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "apps/kibpay/src/polyfills.ts",
            "tsConfig": "./tsconfig.spec.json",
            "environment": "dev",
            "codeCoverage": true,
            "codeCoverageExclude": [
              "test.ts",
              "**/polyfills.ts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "files": [
              "libs/widget-kibpay-data/**/*.ts"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "widget-kibpay": {
      "root": "libs/widget-kibpay",
      "sourceRoot": "libs/widget-kibpay/src",
      "projectType": "library",
      "prefix": "kp",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/widget-kibpay/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "apps/kibpay/src/polyfills.ts",
            "tsConfig": "./tsconfig.spec.json",
            "environment": "dev",
            "codeCoverage": true,
            "codeCoverageExclude": [
              "test.ts",
              "**/polyfills.ts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "files": [
              "libs/widget-kibpay/**/*.ts"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@bb-cli/schematics",
    "analytics": false
  },
  "defaultProject": "kibpay"
}